<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EniBundle\Entity\Lieu" table="Lieu">
    <id name="codelieu" type="integer" column="CodeLieu">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelle" type="string" column="Libelle" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="archive" type="boolean" column="archive" nullable="true"/>
    <field name="gestionemargement" type="boolean" column="GestionEmargement" nullable="true"/>
    <field name="debutam" type="string" column="DebutAM" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="finam" type="string" column="FinAM" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="debutpm" type="string" column="DebutPM" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="finpm" type="string" column="FinPM" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="Adresse" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cp" type="integer" column="CP" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ville" type="string" column="Ville" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
